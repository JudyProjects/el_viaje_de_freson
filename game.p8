-- festino a., sancristobal f.

function _init()
	x=64
	y=64
	s=1
end

function _draw()
	if (btn(⬅️))	x=x-2
	if (btn(➡️))	x=x+2
	if (btn(⬆️)) y=y-2
	if (btn(⬇️))	y=y+2
end

function _update()
	cls(5)
	
	spr(s,x,y,2,2)
end
