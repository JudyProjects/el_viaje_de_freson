-- enemigos
function ini_enemigos()
    enes = {}
end

function make_enemy(num)
    local e = make_entity()
    local upd = e.upd

    e.name = "chorro"
    e.s = 33
    e.fh = false
    e.fr = {
        walk = { 33 },
        idle = { 33 }
    }

    local idle = "idle"
    local walk = "walk"

    local velchorro = 1.5

    e.upd = function()
        e.stat = idle
    end

    e.drw = function()
        local sps = e.fr[e.stat]
        e.s += .15

        if flr(e.s) > #sps then
            e.s = 1
        end
        --fondo transparente
        palt(0, false)
        palt(11, true)
        --dibujar chorro
        spr(
            sps[flr(e.s)],
            e.x + 78,
            e.y,
            2, 2,
            e.fh
        )
        palt()
    end

    add(enes, e)

    return e
end

