-- capas
function ini_bosque()
    arboles = {}
    piedras = {}

    for i = 1, 10 do
        add(
            arboles, {
                x = flr(rnd(128)),
                y = flr(rnd(128)),
                c = 27
            }
        )
    end

    for i = 1, 10 do
        add(
            piedras, {
                x = flr(rnd(128)),
                y = flr(rnd(128)),
                c = 43
            }
        )
    end
end

function drw_bosque()
    for a in all(arboles) do
        a.y += a.c - 4.5

        if (a.y > 140) then
            a.y = -1
            a.x = flr(rnd(128))
        end
    end

    for p in all(piedras) do
        p.y += p.c - 4.5

        if (p.y > 140) then
            p.y = -1
            p.x = flr(rnd(128))
        end
    end
end
